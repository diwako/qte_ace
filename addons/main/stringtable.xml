<?xml version="1.0" encoding="utf-8"?>
<Project name="qte_ace">
    <Package name="main">
        <Key ID="STR_QTE_ACE_main_Category">
            <English>QTE ACE - Main</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_enable">
            <English>Enable QTE</English>
            <French>Activer les QTE</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_enable_desc">
            <English>Enables QTE substitution for the ACE3 module</English>
            <French>Permet la substitution de QTE pour le module ACE3</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_difficulty">
            <English>Difficulty</English>
            <French>Difficultés</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_difficulty_desc">
            <English>Higher values mean more inputs thus higher difficulty, reducing the value means easier QTEs</English>
            <French>Une valeur élevée signifie plus d'entrées et donc une plus grande difficulté, une valeur réduite signifie des QTE plus faciles.</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_qte_type">
            <English>QTE Types</English>
            <French>Types de QTE</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_qte_type_desc">
            <English>Choose which kind of QTEs should be used</English>
            <French>Choisir le type de QTE à utiliser</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_qte_type_all">
            <English>All</English>
            <French>Tous</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_qte_type_arrows">
            <English>Arrows</English>
            <French>Flèches</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_qte_type_words">
            <English>Words and phrases</English>
            <French>Mots et phrases</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_resetUponIncorrectInput">
            <English>Reset on wrong input</English>
            <French>Réinitialisation en cas d'entrée erronée</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_resetUponIncorrectInput_desc">
            <English>Resets the whole QTE if you press one wrong button, that means you need to start over</English>
            <French>Réinitialise l'ensemble du QTE si vous appuyez sur un mauvais bouton, ce qui signifie que vous devez recommencer.</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_tries">
            <English>Amount of tries</English>
            <French>Nombre d'essais</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_tries_desc">
            <English>Each time you press a wrong button you lose a try, if you exceed the number of allowed tries, you fail. Set 0 to have unlimited tries</English>
            <French>Chaque fois que vous appuyez sur un mauvais bouton, vous perdez un essai. Si vous dépassez le nombre d'essais autorisés, vous échouez. Mettez 0 pour avoir un nombre illimité d'essais</French>
        </Key>
        <Key ID="STR_QTE_ACE_main_no_sound">
            <English>No sound</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_correct">
            <English>Sound: Correct</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_correct_desc">
            <English>Sound upon correct button input inside the QTE</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_wrong">
            <English>Sound: Wrong</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_wrong_desc">
            <English>Sound upon wrong button input inside the QTE</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_win">
            <English>Sound: Win</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_win_desc">
            <English>Sound upon successfully clearing the QTE</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_lose">
            <English>Sound: Lose</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_lose_desc">
            <English>Sound upon losing the QTE</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_last_try">
            <English>Sound: Last try</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_last_try_desc">
            <English>Sound when you only have one try left</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_maxLength">
            <English>Max length</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_maxLength_desc">
            <English>Maximum length of a QTE sequence before it just reverts to the normal ACE progress bar</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_qte_must_be_completed">
            <English>QTE must be completed</English>
        </Key>
        <Key ID="STR_QTE_ACE_main_qte_must_be_completed_desc">
            <English>When enabled, the QTE must be successfully completed, else it counts as a failure</English>
        </Key>
    </Package>
</Project>
